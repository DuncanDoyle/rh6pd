    <!-- nodes -->
    <startEvent id="_1" name="Start" />
    <scriptTask id="_2" name="Init" scriptFormat="http://www.java.com/java" >
      <script>System.out.println("start");</script>
    </scriptTask>
    <scriptTask id="_3" name="PrintMe" scriptFormat="http://www.java.com/java" >
      <script>System.out.println("Entering " + kcontext.getNodeInstance().getNodeName() + " Node");
System.out.println("Name is " + name);
System.out.println("Age  is " + age);
risk="Not Set Yet";
price="Not Set Yet";
priceInt=999;
System.out.println("Risk is " + risk );
System.out.println("PriceInt is " + priceInt );

// Calculate Age
ageInt = Integer.parseInt(age);
System.out.println("Actual Age is " + ageInt);

// Create a new Driver
driver = new Driver();
policy = new Policy();

// Set Variables to default values.
policy.setDriverRisk(999);
driver.setCreditScore(999);

// Set driver name and age.
driver.setDriverName(name);
driver.setAge(ageInt);

// insert driver &amp; policy worksheet  variables
kcontext.setVariable("driver",driver);
kcontext.setVariable("policy",policy);
kcontext.setVariable("name",name);
kcontext.setVariable("age",age);
kcontext.setVariable("risk",risk);
kcontext.setVariable("price",price);
kcontext.setVariable("priceInt",priceInt); 

// Insert Driver and Policy into our session for the rules to use.
insert(driver); 
insert(policy);

System.out.println("Driver Name is " + driver.getDriverName() ) ;
System.out.println("Driver Age    is " + driver.getAge() ) ;

System.out.println("Leaving " + kcontext.getNodeInstance().getNodeName() +" Node");</script>
    </scriptTask>
