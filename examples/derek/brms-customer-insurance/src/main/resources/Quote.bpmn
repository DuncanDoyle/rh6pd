    <scriptTask id="_8" name="PrintMeAgain1" scriptFormat="http://www.java.com/java" >
      <script>System.out.println("Start PrintMe Again");
// get policy out of rules engine.
System.out.println("We think policy model is " + kcontext.getVariable("policy"));
policy =  (org.jbpm.evaluation.carinsurance.Policy) kcontext.getVariable("policy");
System.out.println("We think policy model is " + kcontext.getVariable("policy"));
// Get car risk value from policy.
riskInt = policy.getDriverRisk();
System.out.println("Risk Int is " + riskInt);
risk      = riskInt.toString();
System.out.println("Risk is " + risk);

System.out.println("PriceInt is " + priceInt);
price    = priceInt.toString();
System.out.println("Price is " + price);

ageInt = Integer.parseInt(age);
System.out.println("Age Int is " + ageInt);

credit  = driver.getCreditScore();
System.out.println("Credit  is " + credit);

// Print everything out
System.out.println("========== P O L I CY ==========");
System.out.println("Policy is ..... ");
System.out.println("Name is " + name );
System.out.println("Age is "    + age );
System.out.println("Driver Credit Score is " + credit );
System.out.println("Policy  Driver intRisk is " + riskInt );
System.out.println("Policy  Driver String Risk is " + risk );
System.out.println("Policy  PriceString is " + priceInt );
System.out.println("=== E N D === P O L I CY ==========");

// create new policy
policy = new Policy();

// reset driver risk
policy.setDriverRisk(riskInt);

System.out.println("Prepare CalcPremium");
kcontext.setVariable("driver",driver);
kcontext.setVariable("policy",policy);
kcontext.setVariable("name",name);
kcontext.setVariable("age",age);
kcontext.setVariable("risk",risk);
kcontext.setVariable("price",price);
insert(driver);
insert(policy);</script>
    </scriptTask>
