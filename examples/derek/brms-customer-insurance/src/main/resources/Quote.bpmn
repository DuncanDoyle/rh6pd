  <process processType="Private" isExecutable="true" id="org.jbpm.evaluation.carinsurance.Quote" name="Quote157" tns:packageName="org.jbpm.evaluation.carinsurance" >

    <!-- process variables -->
    <property id="name" itemSubjectRef="_nameItem"/>
    <property id="age" itemSubjectRef="_ageItem"/>
    <property id="riskInt" itemSubjectRef="_riskIntItem"/>
    <property id="driver" itemSubjectRef="_driverItem"/>
    <property id="policy" itemSubjectRef="_policyItem"/>
    <property id="ageInt" itemSubjectRef="_ageIntItem"/>
    <property id="credit" itemSubjectRef="_creditItem"/>
