  <process processType="Private" isExecutable="true" id="org.jbpm.evaluation.carinsurance.Quote" name="Quote126" tns:packageName="org.jbpm.evaluation.carinsurance" >

    <!-- process variables -->
    <property id="name" itemSubjectRef="_nameItem"/>
    <property id="age" itemSubjectRef="_ageItem"/>
    <property id="riskInt" itemSubjectRef="_riskIntItem"/>
    <property id="driver" itemSubjectRef="_driverItem"/>
    <property id="policy" itemSubjectRef="_policyItem"/>
    <property id="ageInt" itemSubjectRef="_ageIntItem"/>
    <property id="credit" itemSubjectRef="_creditItem"/>
    <property id="fh1" itemSubjectRef="_fh1Item"/>
    <property id="fh2" itemSubjectRef="_fh2Item"/>
    <property id="price" itemSubjectRef="_priceItem"/>
    <property id="risk" itemSubjectRef="_riskItem"/>

    <!-- nodes -->
    <startEvent id="_1" name="Start" />
    <scriptTask id="_2" name="Init" scriptFormat="http://www.java.com/java" >
      <script>System.out.println("start");</script>
    </scriptTask>
    <scriptTask id="_3" name="PrintMe" scriptFormat="http://www.java.com/java" >
      <script>System.out.println("Entering " + kcontext.getNodeInstance().getNodeName() + " Node");
System.out.println("Name is " + name);
System.out.println("Age  is " + age);
risk="Not Set Yet";
System.out.println("Risk is " + risk );

// Calculate Age
ageInt = Integer.parseInt(age);
System.out.println("Actual Age is " + ageInt);

// Create a new Driver
driver = new Driver();
policy = new Policy();

// Set Variables to default values.
policy.setDriverRisk(999);
driver.setCreditScore(999);

// Set driver name and age.
driver.setDriverName(name);
driver.setAge(ageInt);

// insert driver &amp; policy
kcontext.setVariable("driver",driver);
kcontext.setVariable("policy",policy);

// Insert Driver and Policy into our session for the rules to use.
insert(driver); 
insert(policy);

System.out.println("Driver Name is " + driver.getDriverName() ) ;
System.out.println("Driver Age    is " + driver.getAge() ) ;

System.out.println("Leaving " + kcontext.getNodeInstance().getNodeName() +" Node");</script>
    </scriptTask>
    <userTask id="_4" name="GetDriver" >
      <ioSpecification>
        <dataInput id="_4_TaskNameInput" name="TaskName" />
        <dataInput id="_4_GroupIdInput" name="GroupId" />
        <dataOutput id="_4_NameOutput" name="Name" />
        <dataOutput id="_4_AgeOutput" name="Age" />
        <inputSet>
          <dataInputRefs>_4_TaskNameInput</dataInputRefs>
          <dataInputRefs>_4_GroupIdInput</dataInputRefs>
        </inputSet>
        <outputSet>
          <dataOutputRefs>_4_NameOutput</dataOutputRefs>
          <dataOutputRefs>_4_AgeOutput</dataOutputRefs>
        </outputSet>
      </ioSpecification>
      <dataInputAssociation>
        <targetRef>_4_TaskNameInput</targetRef>
        <assignment>
          <from xsi:type="tFormalExpression">GetDriver</from>
          <to xsi:type="tFormalExpression">_4_TaskNameInput</to>
        </assignment>
      </dataInputAssociation>
      <dataInputAssociation>
        <targetRef>_4_GroupIdInput</targetRef>
        <assignment>
          <from xsi:type="tFormalExpression">HR</from>
          <to xsi:type="tFormalExpression">_4_GroupIdInput</to>
        </assignment>
      </dataInputAssociation>
      <dataOutputAssociation>
        <sourceRef>_4_NameOutput</sourceRef>
        <targetRef>name</targetRef>
      </dataOutputAssociation>
      <dataOutputAssociation>
        <sourceRef>_4_AgeOutput</sourceRef>
        <targetRef>age</targetRef>
      </dataOutputAssociation>
      <potentialOwner>
        <resourceAssignmentExpression>
          <formalExpression>mary</formalExpression>
        </resourceAssignmentExpression>
      </potentialOwner>
    </userTask>
    <businessRuleTask id="_6" name="CalcDriverRisk" g:ruleFlowGroup="calcDriverRisk" >
    </businessRuleTask>
    <endEvent id="_7" name="End" >
        <terminateEventDefinition/>
    </endEvent>
    <scriptTask id="_8" name="PrintMeAgain1" scriptFormat="http://www.java.com/java" >
      <script>// get policy out of rules engine.
System.out.println("We think policy model is " + kcontext.getVariable("policy"));
policy =  (org.jbpm.evaluation.carinsurance.Policy) kcontext.getVariable("policy");

// Get car risk value from policy.
riskInt = policy.getDriverRisk();
risk = riskInt.toString();
ageInt = Integer.parseInt(age);

// Get Credit Score from Driver Object.
credit = driver.getCreditScore();

// Print everything out
System.out.println("========== P O L I CY ==========");
System.out.println("Policy is ..... ");
System.out.println("Name is " + name );
System.out.println("Age is "    + age );
System.out.println("Driver Credit Score is " + credit );
System.out.println("Policy Driver intRisk is " + riskInt );
System.out.println("Policy Driver String Risk is " + risk );
System.out.println("=== E N D === P O L I CY ==========");
</script>
    </scriptTask>
    <businessRuleTask id="_9" name="Retract" g:ruleFlowGroup="retract" >
    </businessRuleTask>
