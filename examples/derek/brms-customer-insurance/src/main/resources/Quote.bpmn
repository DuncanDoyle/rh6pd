    <scriptTask id="_12" name="SetPrice" scriptFormat="http://www.java.com/java" >
      <script>
// get policy out of rules engine.
System.out.println("SetPrice : We think policy model is " + kcontext.getVariable("policy"));
policy =  (org.jbpm.evaluation.carinsurance.Policy) kcontext.getVariable("policy");
System.out.println("SetPrice : We think policy model is " + kcontext.getVariable("policy"));
//policy.setPrice(5555);
priceInt= policy.getPrice();
System.out.println("PriceInt is " + priceInt );
price=priceInt.toString();
System.out.println("Price is " + price );

System.out.println("Policy Risk is " + policy.getDriverRisk());
kcontext.setVariable("name",name);
kcontext.setVariable("age",age);
kcontext.setVariable("risk",risk);
kcontext.setVariable("price",price);
kcontext.setVariable("priceInt",priceInt);</script>
