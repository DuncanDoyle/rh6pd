<html>
<head>
	<title>taskform</title>   
	<link href="/rh6pd/resources/stylesheets/style.css" rel="stylesheet" type="text/css"> 
	<script type="text/javascript" src="/rh6pd/resources/javascript/jquery.js"></script>
</head>
<body>

<script type = " text/javascript" src = "/rh6pd/resources/javascript/brmsValidationJavascript.js"></script>

<img src="/rh6pd/resources/images/newPolicy.jpg" alt="Car Insurance" width="200" height="80" />
	
<fieldset>

	<legend>Policy Details</legend>
	
    
	<form action="complete" method="POST" enctype="multipart/form-data" onsubmit="return taskFormValidatorDriver()">
	<table>
	<tr>
		<td>
			<div><legend> About You </legend></div>
		</td>
		
		<td>
			<div><legend> About Your Car </legend></div>
		</td>
		
		<td>   
		     <legend>Your Price</legend>            		
		</td>
		
	</tr>
	
	<tr>
		<td>
			<label for="name">Name</label>
			<div class="div_texbox">
				<input onchange = "softValidateField('Name')" name="Name" type="text" class="textbox" id="Name" value="${Name}" />
			</div>
		</td>
		
		<td>
			<label for="name">Make</label>
	     	<div class="div_textbox">
	        <input onchange = "softValidateField('Make')" name="Make" type="text" class="textbox" id="Make" value="${Make}" /
	    	</div> 
	    </td>
	    
	    <td>     
	    	<label for="name">UK Pounds</label>
		    <div class="div_textbox">
		         <input onchange = "softValidateField('Price')" name="Price" type="text" class="textbox" id="Price" value="${Price}" /
		    </div>                 
		</td>
	    
	</tr>
	<tr>
		<td>
			<label for="name">Street</label>
			<div class="div_texbox"> 
				<input onchange = "softValidateField('Street')" name="Address1" type="text" class="textbox" id="Street" value="${Address1}"/>
			</div> 
		</td>
		
		<td>     
	    	<label for="name">Model</label>
	     	<div class="div_textbox">
	         	<input onchange = "softValidateField('Model')" name="Model" type="text" class="textbox" id="Model" value="${Model}" /
	     	</div>         	    
		</td>
		
		<td>
		</td>
	
	</tr>
	
	<tr>
		<td>	
			<label for="name">City</label>
			<div class="div_texbox">
				<input onchange = "softValidateField('City')" name="Address2" type="text" class="textbox" id="City" value="${Address2}" />
			</div>
		</td>
		
		<td>	
			<label for="name">Reg</label>
			<div class="div_texbox">
				<input onchange = "softValidateField('Reg')" name="Reg" type="text" class="textbox" id="Reg" value="${Reg}" />
			</div>
		</td>
		
		<td>
		</td>
	</tr>
	
	<tr>
		<td>
		<label for="name">Age</label> 
		<div class="div_texbox"> 
			<input onchange = "softValidateField('Age')" name="Age" type="text" class="textbox" id="Age" value="${Age}" />
		</div>	 
		</td>
	</tr> 
	
	<tr>
		<td> 
	    <label for="name">Driver Risk</label>
	    <div class="div_textbox">
	    	<input onchange = "softValidateField('Risk')" name="Risk" type="text" class="textbox" id="Risk" value="${DriverRisk}" /
	    </div>
		</td>
		
		<td>              	    
	     <label for="name">Car Risk</label>
	     <div class="div_textbox">
	         <input onchange = "softValidateField('CarRisk')" name="CarRisk" type="text" class="textbox" id="CarRisk" value="${CarRisk}" /
	     </div>          
		</td>
		
		<td>
		</td>
	
	</tr>
	
	<tr>
		
		
		
		<td>
		</td>
		
		
	</tr>
	
	<tr>                      
	     <td>
	     	<div class="button_div">
	        	<input id="button" name="Submit" type="submit" value="ACCEPT" class="buttons" />
	     	</div>
	     </td>
    </tr>
	
	</table>
	
    </form> 
    <div class="clear"></div>
    </fieldset>      
</div>
	
</body>
</html>
